{
  "name": "typescript-algorithms",
  "version": "0.1.0",
  "private": true,
  "description": "typescript-algorithms is generated by chore-cli.",
  "scripts": {
    "test": "jest",
    "build": "tsc --outDir dist",
    "start": "tsc --outDir dist --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwfan/typescript-algorithms.git"
  },
  "keywords": [
    "chore",
    "chore-cli"
  ],
  "author": {
    "name": "iwfan",
    "email": "i.wangfancn@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iwfan/typescript-algorithms/issues"
  },
  "homepage": "https://github.com/iwfan/typescript-algorithms#readme",
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,scss,css,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  }
}
